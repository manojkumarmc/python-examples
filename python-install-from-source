yum install zlib zlib-devel openssl openssl-devel 

./configure \
--with-zlib \
--prefix=/usr/local \
--enable-shared \
--enable-unicode=ucs4 \
LD_LIBRARY_PATH=/usr/local/lib64/python35/config

make && make altinstall && make commoninstall

ln -sf /usr/local/lib/libpython3.5m.so /usr/lib64/libpython3.5m.so.1.0
